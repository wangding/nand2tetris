 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=ao);
    Not(in=b, out=bo);
    And(a=ao, b=bo, out=xo);
    Not(in=xo, out=out);
}
